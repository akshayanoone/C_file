#include <stdio.h>

int main() {
    /*To input sides of a triangle and check whether a triangle is equilateral, scalene or isosceles triangle using if else. */
    int a, b,c;
    scanf("%d%d%d",&a,&b,&c);
    {
        if( a==b && b==c)
         printf("triangle is equilateral");
         else
         {
             if(a==b || b==c || a==c)
          printf("triangle is isosceles");
         }
    }
   

    return 0;
}
