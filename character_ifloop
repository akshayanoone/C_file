#include <stdio.h>
int main() {
    /*checking whethere given character is alphabet or not using ifelse loop */
     char ch;
    scanf("%c",&ch);
    {
        if((ch<='a'|| ch>='z') && (ch<='A' || ch>='Z'))
        printf("Given character is alphabet ");
        else
        printf("Given character is not alphabet");
}
    return 0;
}
