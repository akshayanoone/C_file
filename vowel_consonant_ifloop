#include <stdio.h>
int main() {
    /*checking whethere given character is vowel or consonant not using ifelse loop */
     char ch;
     scanf("%c",&ch);
    {
        if(ch=='a'|| ch=='e' || ch=='i' |ch=='o' || ch=='u'||ch=='A'|| ch=='E' || ch=='I' |ch=='O' || ch=='U')
        printf("Given character is vowel ");
        else if((ch>='a'||ch>='z') && (ch>='A'|| ch<='Z'))
        printf("Given character is consonant");
        else
        printf("Given character is not vowel and consonant ");
}
    return 0;
}
